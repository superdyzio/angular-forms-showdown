{
  "name": "angular-forms-showdown",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "i18n:extract": "ng extract-i18n --output-path src/locale --format xlf",
    "serve:pl": "ng serve --configuration pl",
    "build:pl": "ng build --configuration pl"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.0-next.3",
    "@angular/compiler": "^21.0.0-next.3",
    "@angular/core": "^21.0.0-next.3",
    "@angular/forms": "^21.0.0-next.3",
    "@angular/localize": "^21.0.0-next.3",
    "@angular/platform-browser": "^21.0.0-next.3",
    "@angular/router": "^21.0.0-next.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0-next.3",
    "@angular/cli": "^21.0.0-next.3",
    "@angular/compiler-cli": "^21.0.0-next.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
